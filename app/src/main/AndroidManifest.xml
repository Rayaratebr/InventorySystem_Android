<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.inventory_system_android">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:noHistory="false"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".CustomerActivities.ViewCustomerActivity" />
        <activity android:name=".OrderActivities.NewOrderActivity" />
        <activity
            android:name=".Splash"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewProductActivity" />
        <activity android:name=".PaymentActivities.NewPaymentActivity" />
        <activity android:name=".OrderActivities.ChooseProductsActivity" />

        <service android:name=".Service.ScheduleService" />
        <service android:name=".Service.NotifyService" />

        <activity android:name=".OrderActivities.ViewOrderActivity"></activity>
    </application>

</manifest>